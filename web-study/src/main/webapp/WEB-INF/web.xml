<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>web-demo</display-name>
    <servlet>
        <servlet-name>CircleServlet</servlet-name>
        <servlet-class>com.yoveuio.view.CircleServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HelloServlet</servlet-name>
        <servlet-class>com.yoveuio.view.HelloServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LifeServlet</servlet-name>
        <servlet-class> com.yoveuio.view.LifeServlet</servlet-class>
    </servlet>

    <servlet>
        <description>life</description>
        <display-name>LifeServlet</display-name>
        <servlet-name>LifeServletInit</servlet-name>
        <servlet-class> com.yoveuio.view.LifeServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>LifeServletInit</servlet-name>
        <url-pattern>/lifeInit</url-pattern>
    </servlet-mapping>


    <servlet-mapping>
        <servlet-name>HelloServlet</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CircleServlet</servlet-name>
        <url-pattern>/circle</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LifeServlet</servlet-name>
        <url-pattern>/lifes</url-pattern>
        <url-pattern>/life</url-pattern>
    </servlet-mapping>


</web-app>
